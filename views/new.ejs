    <h1 style="text-align:center;">Create a new project<h1>
      <div style="width:30%; margin: 0 auto;">
      <form action="/profile" method="POST">
        <div class="form-group">
          <p style="font-size:20px;">Project you want to choose:</p>
                <select class="form-control" id="projectname" name="projectname" placeholder="project">
                  <option>SHIRTS</option>
                  <option>JEANS</option>
                  <option>BOTTLES</option>
                  <option>SHOES</option>
                  <option>SANDALS</option>
                  <option>HATS</option>
                  <option>PENS</option>
                  <option>WIRES</option>
                  <option>JUGS</option>
                </select>
        <div class="form-group"><p style="font-size:20px;">Email for contact:</p>
            <input class="form-control" type="text" name="emailid" placeholder="emailid"></div>
            <div class="form-group"><p style="font-size:20px;">Phone number for contact:</p>
              <input class="form-control" type="text" name="contact" placeholder="contact"></div>
              <div class="form-group">
                <p style="font-size:20px;">Units you want to maufacture:</p>
                <select class="form-control" id="unit" name="unit" placeholder="unit">
                  <option>10</option>
                  <option>25</option>
                  <option>50</option>
                  <option>100</option>
                  <option>150</option>
                  <option>200</option>
                  <option>250</option>
                  <option>300</option>
                  <option>400</option>
                  <option>500</option>
                  <option>1000</option>
                  <option>1500</option>
                  <option>2000</option>
                  <option>2500</option>
                  <option>5000</option>
                  <option>10000</option>
                </select>
                <div class="form-group"><p style="font-size:20px;">Prototype image:</p>
                  <input class="form-control" type="text" name="image" placeholder="image url"></div>
                  <div class="form-group" ><p style="font-size:20px;">Some description about product:</p>
                <input class="form-control" type="text" name="description" placeholder="description" >
              </div>
              <input type="text" name="author" value="<%= user._id %>" hidden>
        <div class="form-group">
        <button class="btn btn-default btn-lg btn-block btn-success">SUBMIT</button></div>
                </form>





    <!-- jQuery -->
    <script src="js/jquery.js"></script>

